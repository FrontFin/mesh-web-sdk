{
  "name": "@meshconnect/web-link-sdk",
  "version": "2.1.0",
  "description": "A client-side JS library for integrating with Mesh Connect",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "license": "MIT",
  "repository": "https://github.com/FrontFin/mesh-web-sdk.git",
  "homepage": "https://github.com/FrontFin/mesh-web-sdk",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.2.1",
    "ts-jest": "^29.1.2"
  },
  "dependencies": {
    "@meshconnect/node-api": "^2.0.5",
    "@wagmi/core": "^2.10.5",
    "viem": "^2.x",
    "@wagmi/connectors": "^5.0.7"
  },
  "scripts": {
    "build": "yarn updateVersion && rimraf dist && yarn build:esm && yarn build:cjs && yarn copy",
    "build:esm": "tsc --outDir dist",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint **/*.ts",
    "copy": "node ./tools/copy",
    "updateVersion": "node ./tools/update-version",
    "publish:npm": "yarn build && cd dist && yarn publish --access public",
    "test": "jest",
    "test:cov": "jest --ci --coverage"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "coverageDirectory": "./coverage",
    "transform": {
      "\\.[jt]sx?$": "ts-jest"
    },
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    }
  }
}
